[run]
branch = True
source = .
dynamic_context = test_function
omit =
    test_*.py
    code_graph.py
    venv/*
    .venv/*

[report]
show_missing = True
skip_covered = False
precision = 2
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:

[html]
directory = htmlcov
skip_covered = False
show_contexts = True